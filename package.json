{
  "name": "grpc-web-rx",
  "version": "0.6.4",
  "homepage": "https://wisetime.com",
  "main": "dist/index.js",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wisetime-io/grpc-web-rx.git"
  },
  "dependencies": {
    "grpc-web": "^1.4.1",
    "rxjs": "^7.5.7",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.3.7",
    "@types/faker": "^5.5.8",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "factory.ts": "^0.5.2",
    "faker": "^5.5.3",
    "grpc-tools": "^1.11.2",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "jest": "26.4.0",
    "jsdom": "^16.7.0",
    "pm2": "^5.2.0",
    "rxjs-marbles": "^7.0.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "xhr2": "^0.2.1"
  },
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "test": "jest --no-cache",
    "test-local": "make test-generate-proto && npm run start-local-server && sleep 2 && make start-local-proxy && sleep 2 && jest --no-cache && npm run stop-local-server",
    "build": "make test-generate-proto && tsc",
    "eslint": "eslint --quiet src",
    "start-local-server": "pm2 start ts-node -- src/test/integration/server/server.ts",
    "stop-local-server": "pm2 stop ts-node && pm2 delete ts-node"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1,
          "FunctionDeclaration": {
            "parameters": "first"
          }
        }
      ],
      "semi": [
        "error",
        "never"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "prefer-destructuring": 1,
      "react/display-name": 0,
      "@typescript-eslint/member-delimiter-style": [
        "error",
        {
          "multiline": {
            "delimiter": "comma",
            "requireLast": true
          },
          "singleline": {
            "delimiter": "comma",
            "requireLast": false
          }
        }
      ],
      "object-curly-spacing": [
        "error",
        "always",
        {
          "arraysInObjects": false,
          "objectsInObjects": false
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  }
}
